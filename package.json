{
  "name": "maxine-discovery",
  "version": "1.0.0",
  "description": "Maxine is a discovery and a naming server for all the running nodes with gargantua client dependency.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "set profile=dev & nodemon index.js",
    "test": "mocha --timeout 10000 --exit"
  },
  "keywords": [
    "maxine",
    "discovery-server",
    "naming-server"
  ],
  "author": "Vrushank Patel",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.25.0",
    "body-parser": "^1.19.1",
    "date-and-time": "^2.1.0",
    "express": "^4.17.2",
    "express-actuator": "^1.8.0",
    "express-status-monitor": "^1.3.3",
    "ip": "^1.1.5",
    "properties-reader": "^2.2.0",
    "read-last-lines": "^1.8.0",
    "winston": "^3.5.1"
  },
  "nodemonConfig": {
    "ext": "js,properties"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "chai-json": "^1.0.0",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VrushankPatel/Gargantua-Maxine-Server"
  }
}
