{
  "name": "maxine-discovery",
  "version": "1.0.0",
  "description": "Maxine is a discovery and a naming server for all the running nodes with gargantua client dependency.",
  "main": "index.js",
  "scripts": {
    "start": "npm run test && node index.js",
    "dev": "nodemon index.js --env dev",
    "test": "nyc --reporter=html --reporter=lcov npx mocha --timeout 10000 --exit",
    "coverage": "npx codecov -t $CODECOV_TOKEN"
  },
  "keywords": [
    "maxine",
    "discovery-server",
    "naming-server"
  ],
  "author": "Vrushank Patel",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.1",
    "date-and-time": "^2.1.0",
    "express": "^4.17.2",
    "express-actuator": "^1.8.0",
    "express-status-monitor": "^1.3.3",
    "minimist": "^1.2.5",
    "properties-reader": "^2.2.0",
    "read-last-lines": "^1.8.0",
    "winston": "^3.5.1",

    "follow-redirects": ">=1.14.8",
    "axios": ">=0.21.2",
    "ws": ">=6.2.2",
    "xmlhttprequest-ssl": ">=1.6.2",
    "engine.io": ">=4.0.0",
    "socket.io": ">=2.4.0"
  },
  "nodemonConfig": {
    "ext": "js,properties"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "chai-json": "^1.0.0",
    "codecov": "^3.8.3",
    "istanbul": "^0.4.5",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VrushankPatel/Gargantua-Maxine-Server"
  }
}
